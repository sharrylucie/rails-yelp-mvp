<div class="container">
  <h1><strong>Restaurants</strong></h1>
  <div>
    <ul>
      <% @restaurants.each do |restaurant| %>
        <li><%= link_to restaurant.name.capitalize, restaurant_path(restaurant)  %></li>
      <% end %>
    </ul>
  </div>
  <%= link_to "New restaurant", new_restaurant_path(@restaurant)  %>
</div>
